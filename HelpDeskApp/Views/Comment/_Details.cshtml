@model HelpDeskApp.Models.CommentViewModel.CommentCRUDViewModel

<div class="row">
<div class="col-md-12">
<table class="ItemTranTable">
<tr><th>Id</th><td>@Model.Id</td></tr>
<tr><th>Id Zgloszenia</th><td>@Model.ComplaintId</td></tr>
<tr><th>Wiadomosc</th><td>@Model.Message</td></tr>
<tr><th>Usuniety</th><td>@Model.IsDeleted</td></tr>
<tr><th>Data Stworzenia</th><td>@Model.CreatedDate</td></tr>
<tr><th>Data Modyfikacji</th><td>@Model.ModifiedDate</td></tr>
<tr><th>Stworzony przez</th><td>@Model.CreatedBy</td></tr>
<tr><th>Zmodyfikowany przez</th><td>@Model.ModifiedBy</td></tr>
<tr><th>Anulowany</th><td>@Model.Cancelled</td></tr>
</table>
</div>
</div>
